<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/mainTopBar"
        android:layout_width="383dp"
        android:layout_height="74dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp"
        android:layout_alignParentLeft="true" />

    <Button
        android:id="@+id/balancedBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="49dp"
        android:layout_marginTop="160dp"
        android:layout_marginRight="49dp"
        android:layout_alignParentRight="true" />

    <Button
        android:id="@+id/ketoBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/balancedBtn"
        android:layout_marginStart="58dp"
        android:layout_marginLeft="58dp"
        android:layout_alignParentLeft="true" />

    <Button
        android:id="@+id/bulkBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/ketoBtn"
        android:layout_marginBottom="223dp" />

    <Button
        android:id="@+id/paleoBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/balancedBtn"
        android:layout_alignTop="@+id/bulkBtn" />

</RelativeLayout>